{
  "name": "react-virtualized",
  "description": "React components for efficiently rendering large, scrollable lists and tabular data",
  "author": "Brian Vaughn <brian.david.vaughn@gmail.com>",
  "user": "bvaughn",
  "version": "9.10.0",
  "homepage": "https://github.com/bvaughn/react-virtualized",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:css && npm run build:es && npm run build:demo && npm run build:umd",
    "build:commonjs": "npm run clean:commonjs && cross-env NODE_ENV=production cross-env BABEL_ENV=commonjs babel source --out-dir dist/commonjs --ignore *.example.js,*.test.js,source/demo/,source/tests.js",
    "build:css": "postcss source/styles.css --config postcss.config.js --use autoprefixer -o styles.css",
    "build:demo": "npm run clean:demo && cross-env NODE_ENV=production webpack --config webpack.config.demo.js -p --bail",
    "build:es": "npm run clean:es && cross-env NODE_ENV=production cross-env BABEL_ENV=es babel source --out-dir dist/es --ignore *.example.js,*.test.js,source/demo/,source/tests.js",
    "build:umd": "npm run clean:umd && cross-env NODE_ENV=production webpack --config webpack.config.umd.js --bail",
    "clean": "npm run clean:commonjs && npm run clean:demo && npm run clean:es && npm run clean:umd",
    "clean:commonjs": "rimraf dist/commonjs",
    "clean:demo": "rimraf build",
    "clean:es": "rimraf dist/es",
    "clean:umd": "rimraf dist/umd",
    "deploy": "gh-pages -d build",
    "lint": "eslint 'source/**/*.js'",
    "typecheck": "flow check",
    "prebuild": "npm run typecheck && npm run lint",
    "predeploy": "cp ./circle.yml ./build/",
    "prettier": "prettier --write 'source/**/*.js'",
    "postpublish": "npm run deploy",
    "posttest": "[ -z \"$CI\" ] || codecov",
    "prepublish": "npm run build",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config webpack.config.dev.js",
    "test": "npm run test:jest",
    "test:jest": "jest --no-watchman --runInBand",
    "watch": "watch 'clear && npm run test -s' source",
    "watch:jest": "jest --no-watchman --watch"
  },
  "pre-commit": [
    "typecheck",
    "prettier",
    "lint"
  ],
  "files": [
    "dist",
    "styles.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bvaughn/react-virtualized.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "virtual",
    "list",
    "scrolling",
    "infinite",
    "virtualized",
    "table",
    "fixed",
    "header",
    "flex",
    "flexbox",
    "grid",
    "spreadsheet"
  ],
  "bugs": {
    "url": "https://github.com/bvaughn/react-virtualized/issues"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist",
      "playground",
      "source/vendor"
    ],
    "global": [
      "afterAll",
      "afterEach",
      "beforeAll",
      "beforeEach",
      "describe",
      "expect",
      "fdescribe",
      "fit",
      "getComputedStyle",
      "HTMLElement",
      "it",
      "jest",
      "spyOn"
    ]
  },
  "jest": {
    "setupFiles": [
      "./source/jest-setup.js"
    ],
    "roots": [
      "./source"
    ],
    "testRegex": ".jest.js",
    "verbose": true
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-flow-react-proptypes": "^3.4.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bluebird": "^3.5.0",
    "codecov": "^2.2.0",
    "codemirror": "^5.27.4",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.50.0",
    "fs-extra": "^4.0.0",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.29.0",
    "immutable": "^3.8.1",
    "jest": "^20.0.4",
    "jest-changed-files": "^20.0.3",
    "jest-cli": "^20.0.4",
    "jest-config": "^20.0.4",
    "jest-diff": "^20.0.3",
    "jest-environment-jsdom": "^20.0.3",
    "jest-jasmine2": "^20.0.4",
    "jest-matcher-utils": "^20.0.3",
    "jest-matchers": "^20.0.3",
    "jest-mock": "^20.0.3",
    "jest-resolve": "^20.0.4",
    "jest-resolve-dependencies": "^20.0.3",
    "jest-snapshot": "^20.0.3",
    "jest-util": "^20.0.3",
    "postcss": "^6.0.7",
    "postcss-cli": "^4.1.0",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.3",
    "pretty-format": "^20.0.3",
    "raf": "^3.3.2",
    "react": "next",
    "react-codemirror": "^1.0.0",
    "react-dom": "next",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.4.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "watch": "^1.0.2",
    "webpack": "^3.3.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "dom-helpers": "^3.2.1",
    "loose-envify": "^1.3.1",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0-alpha",
    "react-dom": "^15.3.0 || ^16.0.0-alpha"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}